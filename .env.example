# Example environment configuration file
# Copy this file to .env and update with your values

# Debug mode
DEBUG=false

# Algorithm parameters
DEFAULT_ALGORITHM=barrier-breaker
ADAPTIVE_MODE=true

# Performance tuning
MAX_HEAP_SIZE=10000
CLUSTER_SIZE_HINT=auto
LAYER_GRANULARITY=auto

# Benchmarking
BENCHMARK_SEED=42
BENCHMARK_OUTPUT_DIR=./results

# Testing
TEST_VERBOSE=false
TEST_PARALLEL=true